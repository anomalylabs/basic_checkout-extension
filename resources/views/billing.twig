{% extends layout('checkout') %}

{% block content %}
    <section id="checkout">

        <h2>Checkout [billing]</h2>

        <br><br>

        {{ form('checkout.billing')
        .entry(checkout.cart_id)
        .successMessage(false)
        .redirect('checkout/complete/' ~ checkout.str_id.value)|raw }}

    </section>
{% endblock %}
